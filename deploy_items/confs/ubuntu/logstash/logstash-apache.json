            "keyword": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        },
        "request": {
          "type": "text",
          "index": "not_analyzed",
          "fields": {
            "keyword": {
              "type": "keyword",
              "ignore_above": 256
